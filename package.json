{
  "name": "inacio-luma-patient-recommender",
  "version": "0.9.0",
  "description": "This library recommends a list of patients most likely to pick up a call from the hospital, based on the hospital's location and other optional parameters.",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:docker": "docker build . -t backend-interview-inacio",
    "start": "cross-env NODE_ENV=production node dist/http-api.js",
    "start:docker": "docker run -p 3000:3000 backend-interview-inacio",
    "dev": "cross-env NODE_ENV=dev nodemon --watch 'src/**/*.ts' --exec 'ts-node --transpile-only' src/http-api.ts",
    "dev:debug": "cross-env NODE_ENV=debug nodemon --watch 'src/**/*.ts' --exec 'ts-node --transpile-only' src/http-api.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "prepare": "npx husky install",
    "test": "cross-env SERVER_PORT=3333 jest src --config jest.config.js",
    "test:coverage": "cross-env SERVER_PORT=3333 jest src --config jest.config.js --coverage",
    "test:e2e": "cross-env NODE_ENV=dev cross-env SERVER_PORT=3333 jest src --config jest.config.e2e.js",
    "test:mutant": "cross-env NODE_ENV=production cross-env SERVER_PORT=3333 stryker run",
    "load": "artillery run --output ./stress/recommend-patients.stress-results.json ./stress/recommend-patients.stress-test.yml && artillery report ./stress/recommend-patients.stress-results.json",
    "stress": "wrk -t12 -c400 -d30s -s stress/recommend-patients-stress.lua http://localhost:3000"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@stryker-mutator/jest-runner": "^8.5.0",
    "@types/jest": "^29.5.12",
    "@types/kd-tree-javascript": "^1.0.3",
    "@types/node": "^22.3.0",
    "@types/static-kdtree": "^1.0.3",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "artillery": "^2.0.19",
    "eslint": "^9.9.0",
    "globals": "^15.9.0",
    "husky": "^9.1.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.4",
    "pino-pretty": "^11.2.2",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript-eslint": "^8.1.0"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "@fastify/one-line-logger": "^1.4.0",
    "@fastify/swagger": "^8.15.0",
    "@fastify/swagger-ui": "^4.1.0",
    "cross-env": "^7.0.3",
    "fastify": "^4.28.1",
    "fastify-healthcheck": "^4.4.0",
    "kd-tree-javascript": "^1.0.3",
    "typescript": "^5.5.4",
    "zod": "^3.23.8"
  }
}
