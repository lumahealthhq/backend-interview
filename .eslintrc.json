{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "prefer-arrow-callback": "off",
        "func-names": "off",
        "no-param-reassign": "off",
        "class-methods-use-this": "off"
    }
}
